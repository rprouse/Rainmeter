[Rainmeter]
Update=1000
AccurateText=1
DynamicWindowSize=1
SkinWidth=300
SkinHeight=310
@include=#@#Variables.inc

[MeasureGPU]
Measure=Plugin
Plugin=MSIAfterburner.dll
DataSource=GPU Usage

[MeasureGPUHistory]
Measure=Calc
Formula=MeasureGPU
HistoryCount=60

[MeterGPUHistoryGlow]
Meter=Line
MeasureName=MeasureGPUHistory
X=10
Y=170
W=280
H=60
LineColor=#GraphColor#22
LineWidth=5
AntiAlias=1

[MeterGPUHistory]
Meter=Line
MeasureName=MeasureGPUHistory
X=10
Y=170
W=280
H=60
LineColor=#GraphColor#
LineWidth=2
AntiAlias=1

[MeterLabelGPU_Glow]
Meter=String
Text=GPU USAGE
FontColor=#GraphColor#33
FontSize=#FontSize#
FontFace=#FontName#
X=10
Y=230
AntiAlias=1

[MeterLabelGPU]
Meter=String
Text=GPU USAGE
FontColor=#GraphColor#
FontSize=#FontSize#
FontFace=#FontName#
X=10
Y=230
AntiAlias=1